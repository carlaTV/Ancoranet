<lexentry lemma="saltar" lng="es" type="verb">
  <sense id="1">
    <frame default="yes" lss="B11.unaccusative-motion" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="arg4" function="cc" thematicrole="des">
        <constituent preposition="a" type="sp"/>
      </argument>
      <argument argument="argM" function="cpred" thematicrole="atr"/>
      <argument argument="argM" function="cc" thematicrole="tmp">
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
*0* Saltaron juntos a la NBA en 1992
        </example>
        <example>
Luego *0* saltaste a otra Europa
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="2">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="ext"/>
      <examples>
        <example>
saltar el muro
        </example>
        <example>
La barrera resultaba fácil de saltar
        </example>
        <example>
aspiraba a saltar entre los 8,70 y los 8,75 metros
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="3">
    <frame default="yes" lss="C41.state-benefactive" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="arg2" function="ci" thematicrole="ben"/>
      <argument argument="arg3" function="cc" thematicrole="ori">
        <constituent preposition="de" type="sp"/>
      </argument>
      <examples>
        <example>
aunque el corazón se les saltara del pecho
        </example>
        <example>
el suicida leyó algunas páginas antes de saltar
        </example>
      </examples>
    </frame>
    <frame lss="A11.transitive-causative" type="causative">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <argument argument="arg2" function="ci" thematicrole="ben">
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
hacer saltar lágrimas a gentes
        </example>
        <example>
hacer saltar las ronchas
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="4">
    <frame default="yes" lss="D11.inergative-agentive" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg3" function="cc" thematicrole="ori">
        <constituent preposition="de" type="sp"/>
        <constituent preposition="desde" type="sp"/>
      </argument>
      <argument argument="arg4" function="cc" thematicrole="des">
        <constituent preposition="hacia" type="sp"/>
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
Ginés saltó del vehículo
        </example>
        <example>
para poder saltar nuevamente al ring
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="5">
    <frame default="yes" lss="C11.state-existential" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <examples>
        <example>
La alarma saltó justo antes de Navidades
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="6">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="argL" function="cd"/>
      <examples>
        <example>
propone saltarse una generación de tecnología
        </example>
        <example>
lo que ha hecho el nuevo rey es saltarse el sistema
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="7">
    <frame default="yes" lss="C11.state-existential" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="argL" function="cc"/>
      <examples>
        <example>
Si no actúa pronto, todo saltará por los aires
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="8">
    <frame default="yes" lss="C11.state-existential" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="argL" function="creg">
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
Las tetas saltaron a la palestra
        </example>
        <example>
el Valencia saltó a la fama
        </example>
      </examples>
    </frame>
  </sense>
</lexentry>
