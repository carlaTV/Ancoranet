<lexentry lemma="bajar" lng="es" type="verb">
  <sense id="1">
    <frame default="yes" lss="B11.unaccusative-motion" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="arg2" function="cd" functype="quantitative" thematicrole="ext"/>
      <argument argument="arg3" function="cc" thematicrole="ori">
        <constituent preposition="de" type="sp"/>
      </argument>
      <argument argument="arg4" function="cc" thematicrole="des">
        <constituent preposition="a" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="mnr">
        <constituent preposition="de" type="sp"/>
        <constituent preposition="sin" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="fin">
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
Bajó de los Andes
        </example>
        <example>
Sin esperar a vestirme , *0* bajé en busca del portero del edificio
        </example>
        <example>
Livia y Druso bajaron de las alturas como Dios les dio a entender
        </example>
        <example>
Acumular valor para bajar de una vez el último peldaño
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="2">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="mnr"/>
      <examples>
        <example>
Varios barcelonistas bajaron ostensiblemente los brazos
        </example>
        <example>
Bajar la persiana
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="3">
    <frame default="yes" lss="A11.transitive-causative" type="default">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <argument argument="arg2" function="cd" functype="quantitative" thematicrole="ext"/>
      <argument argument="arg3" function="cc" thematicrole="ein"/>
      <argument argument="arg4" function="cc" thematicrole="efi"/>
      <argument argument="argM" function="cc" thematicrole="ext">
        <constituent preposition="en" type="sp"/>
        <constituent preposition="a" type="sp"/>
        <constituent preposition="hasta" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="loc">
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
Estas medidas bajarán el gasto público
        </example>
        <example>
que hizo bajar el marcador de mercurio de los termómetros hasta los 20 grados en algunos lugares
        </example>
      </examples>
    </frame>
    <frame lss="A11.transitive-causative" type="benefactive">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <argument argument="arg2" function="cd" functype="quantitative" thematicrole="ext"/>
      <argument argument="arg3" function="ci" thematicrole="ben">
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
su propuesta para bajar el impuesto a todo el país
        </example>
      </examples>
    </frame>
    <frame lss="B21.unaccusative-state" type="anticausative">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="arg2" function="cd" functype="quantitative" thematicrole="ext"/>
      <argument argument="arg3" function="cc" thematicrole="ein"/>
      <argument argument="arg4" function="cc" thematicrole="efi"/>
      <argument argument="argM" function="cc" thematicrole="tmp">
        <constituent preposition="en" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="loc">
        <constituent preposition="en" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="ext">
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
El paro registrado bajó en mayo_pasado en 47.689 personas
        </example>
        <example>
el descenso del paro bajara más
        </example>
        <example>
en el conjunto de España *0* bajó un 3,2_por_ciento
        </example>
        <example>
en los últimos doce meses *0* bajó en 117.951 personas
        </example>
        <example>
pero *0* bajó en los servicios
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="4">
    <frame default="yes" lss="D11.inergative-agentive" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="argL" function="cd"/>
      <examples>
