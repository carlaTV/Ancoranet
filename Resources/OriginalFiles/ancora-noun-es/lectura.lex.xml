  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="leer" originlink="verb.leer.1" wordnetsynset="16:05362809">
    <frame appearsinplural="yes" type="default">
      <argument argument="arg0" thematicrole="agt">
