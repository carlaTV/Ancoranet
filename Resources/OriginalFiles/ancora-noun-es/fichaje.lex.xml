      <argument argument="argM" thematicrole="fin">
        <constituent frequency="1" preposition="para" type="sp"/>
      </argument>
      <specifiers>
        <constituent frequency="4" postype="article" type="determiner"/>
        <constituent frequency="1" postype="indefinite" type="determiner"/>
