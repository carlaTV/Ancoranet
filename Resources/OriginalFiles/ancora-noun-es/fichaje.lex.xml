<lexentry lemma="fichaje" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="fichar" originlink="verb.fichar.1" wordnetsynset="16:07522867">
    <frame appearsinplural="yes" type="default">
      <argument argument="arg1" thematicrole="pat">
        <constituent frequency="1" preposition="de" type="sp"/>
