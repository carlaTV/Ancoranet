<lexentry lemma="depreciación" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="depreciar" originlink="verb.depreciar.1" wordnetsynset="16:00234534">
