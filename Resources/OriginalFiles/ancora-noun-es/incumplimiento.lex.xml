<lexentry lemma="incumplimiento" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="incumplir" originlink="verb.incumplir.1" wordnetsynset="16:00040608">
    <frame appearsinplural="no" type="default">
      <argument argument="arg1" thematicrole="tem">
