<lexentry lemma="mover" lng="es" type="verb">
  <sense id="1">
    <frame default="yes" lss="B11.unaccusative-motion" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="arg2" function="cc" thematicrole="loc">
        <constituent preposition="en" type="sp"/>
      </argument>
      <argument argument="arg4" function="cc" thematicrole="des">
        <constituent preposition="hacia" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="mnr"/>
      <examples>
        <example>
el laboratorio en caída libre se movía hacia el receptor
        </example>
        <example>
que se movía lentamente
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="2">
    <frame default="yes" lss="A31.ditransitive-patient-locative" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="arg2" function="cc" thematicrole="loc">
        <constituent preposition="por" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="adv">
        <constituent preposition="con" type="sp"/>
      </argument>
      <examples>
        <example>
*0* no nos movelán
        </example>
        <example>
las naves que movía el viento por aguas y mares con estimable eficiencia
        </example>
      </examples>
    </frame>
    <frame lss="B12.unaccusative-passive-ditransitive" type="passive">
      <argument argument="arg1" function="suj" thematicrole="pat"/>
      <argument argument="arg0" function="cag" thematicrole="agt">
        <constituent preposition="por" type="sp"/>
      </argument>
      <argument argument="arg2" function="cc" thematicrole="loc">
        <constituent preposition="por" type="sp"/>
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
movido por un motor de gas
        </example>
        <example>
el volumen de pasaje y de mercancías que hoy se está moviendo
        </example>
        <example>
el escenario en que se mueva nuestra relación con Venezuela
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="3">
    <frame default="yes" lss="A12.ditransitive-causative-state" type="default">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <argument argument="arg2" function="creg" thematicrole="efi">
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
resultó ser de los que mueven a la reflexión
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="4">
    <frame default="yes" lss="D11.inergative-agentive" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="argL" function="cd"/>
      <examples>
        <example>
nadie movía un dedo
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="5">
    <frame default="yes" lss="D11.inergative-agentive" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="argL" function="cd"/>
      <examples>
        <example>
qué ficha mueve " la coalición que lidera Jordi_Pujol .
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="6">
    <frame default="yes" lss="A11.transitive-causative" type="default">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <examples>
        <example>
cuando él empezaba a hablar o mover la cabeza yo ya sabía lo que quería
        </example>
      </examples>
    </frame>
  </sense>
</lexentry>
