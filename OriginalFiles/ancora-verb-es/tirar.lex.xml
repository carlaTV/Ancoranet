<lexentry lemma="tirar" lng="es" type="verb">
  <sense id="1">
    <frame default="yes" lss="A31.ditransitive-patient-locative" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="arg2" function="ci" thematicrole="ben"/>
      <argument argument="arg3" function="cc" thematicrole="ori">
        <constituent preposition="desde" type="sp"/>
      </argument>
      <argument argument="arg4" function="cc" thematicrole="des">
        <constituent preposition="a" type="sp"/>
        <constituent preposition="en" type="sp"/>
      </argument>
      <argument argument="argM" function="cc" thematicrole="mnr"/>
      <examples>
        <example>
tirar papeleras
        </example>
        <example>
*0* tira muy duro
        </example>
        <example>
ningún blanco tiraba
        </example>
        <example>
usar los palillos una sola vez y tirarlos
        </example>
        <example>
ahora , María_Angélica , tire al cubo de la basura uno de los dos Jesuses
        </example>
        <example>
tiró al desdichado a la calle desde el cuarto piso
        </example>
        <example>
por tirar piedras a la policía
        </example>
      </examples>
    </frame>
    <frame lss="B12.unaccusative-passive-ditransitive" type="passive">
      <argument argument="arg1" function="suj" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="loc"/>
      <examples>
        <example>
que no se tirase el agua de la bolsa de agua caliente
        </example>
        <example>
aquí no se tira nada
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="2">
    <frame default="yes" lss="D11.inergative-agentive" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <examples>
        <example>
*0* no tiró
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="3">
    <frame default="yes" lss="A22.transitive-agentive-theme" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="creg" thematicrole="tem">
        <constituent preposition="de" type="sp"/>
      </argument>
      <examples>
        <example>
que tirase de él
        </example>
        <example>
Esto demuestra que Kohl sigue intentando tirar de los hilos
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="4">
    <frame default="yes" lss="B21.unaccusative-state" type="default">
      <argument argument="arg1" function="suj" thematicrole="tem"/>
      <argument argument="argL" function="cd"/>
      <examples>
        <example>
no estaba dispuesto a tirar la toalla
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="5">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="arg2" function="cc" thematicrole="atr"/>
      <argument argument="argM" function="cc" thematicrole="mnr"/>
      <examples>
        <example>
Nos tiramos todo el verano pasado cantándola en el coche
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="6">
    <frame default="yes" lss="A11.transitive-causative" type="default">
      <argument argument="arg0" function="suj" thematicrole="cau"/>
      <argument argument="argL" function="cc"/>
      <argument argument="arg1" function="cd" thematicrole="tem"/>
      <examples>
        <example>
Hay que tirar por tierra tópicos como el de 'me_lo_van_a_quitar_de_las_manos'
        </example>
      </examples>
    </frame>
  </sense>
</lexentry>
