<lexentry lemma="marcar" lng="es" type="verb">
  <sense id="1">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <examples>
        <example>
marcar un número de teléfono
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="2">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="adv">
        <constituent preposition="para" type="sp"/>
      </argument>
      <examples>
        <example>
Para abreviar , el Real marcó - de_churro - y de penalti
        </example>
      </examples>
    </frame>
    <frame lss="A21.transitive-agentive-patient" type="benefactive">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="arg2" function="ci" thematicrole="ben">
        <constituent preposition="a" type="sp"/>
      </argument>
      <examples>
        <example>
el tanto que *0* marcó hoy al Extremadura
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="3">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="loc">
        <constituent preposition="en" type="sp"/>
      </argument>
      <examples>
        <example>
marcarles un número en el brazo
        </example>
      </examples>
    </frame>
  </sense>
  <sense id="4">
    <frame default="yes" lss="A21.transitive-agentive-patient" type="default">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="argM" function="cc" thematicrole="adv">
        <constituent preposition="sobre" type="sp"/>
      </argument>
      <examples>
        <example>
*0* marcará la pauta sobre el inmediato futuro político
        </example>
        <example>
jugadores que marcarían diferencias
        </example>
      </examples>
    </frame>
    <frame lss="A21.transitive-agentive-patient" type="benefactive">
      <argument argument="arg0" function="suj" thematicrole="agt"/>
      <argument argument="arg1" function="cd" thematicrole="pat"/>
      <argument argument="arg2" function="ci" thematicrole="ben"/>
      <examples>
        <example>
los requisitos que le marca el Plan_General_Metropolitano
        </example>
      </examples>
    </frame>
    <frame lss="B22.unaccusative-passive-transitive" type="passive">
      <argument argument="arg1" function="suj" thematicrole="pat"/>
      <argument argument="arg0" function="cag" thematicrole="agt">
        <constituent preposition="por" type="sp"/>
      </argument>
      <examples>
        <example>
emocionante y marcado por los episodios más que por el buen juego
        </example>
      </examples>
    </frame>
  </sense>
</lexentry>
