<lexentry lemma="conducción" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="conducir" originlink="verb.conducir.3" wordnetsynset="16:00193637">
    <frame appearsinplural="yes" type="default">
      <argument argument="arg1" thematicrole="pat">
        <constituent frequency="1" type="s.a"/>
      </argument>
      <argument argument="argM" thematicrole="cau">
