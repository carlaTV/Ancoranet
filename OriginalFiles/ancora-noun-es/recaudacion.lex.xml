<lexentry lemma="recaudación" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="result" id="1" lexicalized="no" originlemma="recaudar" originlink="verb.recaudar.1" wordnetsynset="16:00656313">
