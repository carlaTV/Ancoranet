  <sense cousin="no" denotation="result" id="2" lexicalized="no" originlemma="promover" originlink="verb.promover.1" wordnetsynset="16:05409803">
    <frame appearsinplural="no" type="default">
      <argument argument="arg0" thematicrole="agt">
        <constituent frequency="1" type="s.a"/>
      </argument>
      <argument argument="arg1" thematicrole="pat">
        <constituent frequency="3" preposition="de" type="sp"/>
      </argument>
      <specifiers>
        <constituent frequency="3" type="void"/>
        <constituent frequency="3" postype="article" type="determiner"/>
        <constituent frequency="1" postype="indefinite" type="determiner"/>
      </specifiers>
      <reference_modifiers>
        <constituent frequency="3" type="s.a"/>
        <constituent frequency="1" type="S"/>
      </reference_modifiers>
      <examples>
        <example file="CESS-CAST-A/1687_20000503.tbf.xml" nodepath="5.3.0.1.1.0.0" sentencenodepath="5">AOL , que tiene previsto fusionarse con el gigante de los medios de comunicación Time_Warner , ha firmado en los últimos meses acuerdos de <argset><head>promoción </head></argset>con otras grandes compañías , como Sears , Wal-Mart y General_Motors . </example>
        <example file="CESS-CAST-P/129_20000702.tbf.xml" nodepath="1.0.1.1" sentencenodepath="1"><argset><specifier>La</specifier><reference_modifiers>séptima</reference_modifiers><head>promoción </head></argset>estará compuesta por 84 alumnos , que seguirán los 16 módulos del programa que se imparten en 20 meses . </example>
        <example file="CESS-CAST-P/136_20001001.tbf.xml" nodepath="48.2.1.1" sentencenodepath="48">Ø Tenemos <argset><specifier>una</specifier><reference_modifiers>larga</reference_modifiers><head>promoción </head><argument argument="arg1" function="cn" thematicrole="pat" type="sp">de la película</argument></argset>. </example>
        <example file="CESS-CAST-P/70_20020102.tbf.xml" nodepath="1.2.1.0.1.1.0" sentencenodepath="1">Todos estos viajes estarán centrados en <argset><specifier>la</specifier><head>promoción </head><argument argument="arg0" function="cn" thematicrole="agt" type="s.a">empresarial</argument><argument argument="arg1" function="cn" thematicrole="pat" type="sp">de Catalunya</argument></argset>y en su difusión como destino de inversiones extranjeras . </example>
        <example file="CESS-CAST-P/72_19990601.tbf.xml" nodepath="8.4.1.1.2.1.1.0" sentencenodepath="8">Para ambos representantes municipales , la vía de comunicación " es muy necesaria , en_especial para <argset><specifier>la</specifier><head>promoción </head><reference_modifiers>turística</reference_modifiers><argument argument="arg1" function="cn" thematicrole="pat" type="sp">de la zona</argument></argset>" . </example>
        <example file="CESS-CAST-P/76_19981201.tbf.xml" nodepath="2.3.1.1.2.0" sentencenodepath="2">De los 22.800 amarres que hay en la costa catalana , la nueva empresa tendrá bajo <argset>su control y <head>promoción </head>
          </argset>8.245 puntos de atraque . </example>
        <example file="CESS-CAST-P/85_19990302.tbf.xml" nodepath="5.2.0.1.1.1.1.0.0" sentencenodepath="5">La operación permitirá construir 330 pisos de <argset><head>promoción </head><reference_modifiers>privada</reference_modifiers></argset>y habilitar 8.000 metros_cuadrados de zonas verdes . </example>
      </examples>
    </frame>
  </sense>
  <sense cousin="no" denotation="event" id="3" lexicalized="no" originlemma="promover" originlink="verb.promover.1" wordnetsynset="16:05412651">
