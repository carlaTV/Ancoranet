<lexentry lemma="distinción" lng="es" origin="deverbal" type="noun">
  <sense cousin="no" denotation="event" id="1" lexicalized="no" originlemma="distinguir" originlink="verb.distinguir.1" wordnetsynset="16:03731593+16:04434776">
