      <specifiers>
        <constituent frequency="2" postype="article" type="determiner"/>
      </specifiers>
      <examples>
        <example file="3LB-CAST/e2-1.tbf.xml" nodepath="7.3.1.2.2.0" sentencenodepath="7">Por_otra_parte Malinowski hace una interesante aportación <argset>: <specifier>la</specifier>
            <head>distinción </head>
            <argument argument="arg1" function="cn" thematicrole="tem" type="sp">entre tribu-nación y tribu-estado</argument>
          </argset>. </example>
        <example file="3LB-CAST/e3-2.tbf.xml" nodepath="3.2.1.1.2.3.1.2.1.1.0" sentencenodepath="3">En 1708 era en las tres dimensiones del cuerpo propio donde Ø ponía el primer fundamento de <argset><specifier>la</specifier><head>distinción </head><argument argument="arg1" function="cn" thematicrole="tem" type="sp">de las regiones</argument><argument argument="argM" function="cn" thematicrole="loc" type="sp">en el espacio</argument></argset>. </example>
      </examples>
    </frame>
  </sense>
  <sense cousin="no" denotation="result" id="3" lexicalized="no" originlemma="distinguir" originlink="verb.distinguir.2" wordnetsynset="16:04434776">
